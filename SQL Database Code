MySQL Code used to make database for this assessment. It would be necessary to change at times I think. Nevertheless, code is below 

create database mdhsDB;
use mdhsDB;

CREATE TABLE customers(
	customer_id INT NOT NULL AUTO_INCREMENT,
	first_Name VARCHAR(40) NOT NULL, 
	last_Name VARCHAR(40) NOT NULL,
	username VARCHAR(40) NOT NULL,
	mobile VARCHAR(15) NOT NULL,
	email VARCHAR(50) NOT NULL,
	address VARCHAR(75) NOT NULL,
  password_field VARCHAR(4000) NOT NULL,
  administrator INT NOT NULL, 
	PRIMARY KEY(customer_id)
);

CREATE TABLE products( 
	product_id INT NOT NULL AUTO_INCREMENT, 
  product_name VARCHAR(50) NOT NULL, 
  unit VARCHAR(20) NOT NULL, 
  quantity INT NOT NULL, 
  price DECIMAL(2,0) NOT NULL,
  ingredients VARCHAR(200),
  PRIMARY KEY(product_id)
); 

CREATE TABLE orders( 
	order_id INT NOT NULL AUTO_INCREMENT, 
  customer_id INT NOT NULL, 
  PRIMARY KEY(order_id), 
  FOREIGN KEY(customer_id) REFERENCES products(product_id)
); 

CREATE TABLE order_contents( 
	order_id INT NOT NULL, 
  product_id INT NOT NULL, 
  quantity INT NOT NULL, 
  FOREIGN KEY(order_id) REFERENCES orders(order_id), 
  FOREIGN KEY(product_id) REFERENCES products(product_id) 
); 

CREATE TABLE delivery_schedule( 
	delivery_id INT NOT NULL AUTO_INCREMENT,
  customer_id INT NOT NULL, 
  order_id INT NOT NULL,
  delivery_day DATE NOT NULL, 
  delivery_cost DECIMAL(2,0) NOT NULL,
  delivery_time TIME NOT NULL,
  PRIMARY KEY (delivery_id),
  FOREIGN KEY (customer_id) REFERENCES customers(customer_id),
  FOREIGN KEY (order_id) REFERENCES orders(order_id)
);
