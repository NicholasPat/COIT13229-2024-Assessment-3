<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="MainPackage.LoginController">
   <children>
      <Button layoutX="156.0" layoutY="261.0" mnemonicParsing="false" onAction="#clickToLogin" text="Login" />
      <Button layoutX="313.0" layoutY="261.0" mnemonicParsing="false" onAction="#clickToRegister" text="Register" />
      <PasswordField fx:id="passwordField" layoutX="300.0" layoutY="188.0" promptText="Please Input Password" />
      <TextField fx:id="usernameField" layoutX="300.0" layoutY="136.0" promptText="Please Input Username" />
   </children>
</AnchorPane>
